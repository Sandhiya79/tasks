#Python program to illustrate create, write and append the data

file1 = open("30days 30hours operations.txt", "w") 

L = ["I have completed 10 days successfully\n"] 
file1.writelines(L) 
file1.close() 

   
# Append-adds at last 

file1 = open("myfile.txt", "a")  # append mode 

file1.write("Sandhiya.K\n") 
file1.close() 

   

file1 = open("myfile.txt", "r") 

print("Output of Readlines after appending") 

print(file1.read()) 

print() 
file1.close() 
